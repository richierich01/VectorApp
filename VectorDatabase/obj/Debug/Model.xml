		<Element Type="SqlScalarFunction" Name="[dbo].[ufn_GetCourseByStudentId]">
			<Property Name="IsAnsiNullsOn" Value="True" />
			<Relationship Name="BodyDependencies">
				<Entry>
					<References ExternalSource="BuiltIns" Name="[varchar]" />
